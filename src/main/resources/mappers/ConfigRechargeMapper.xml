<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ep.dao.mapper.TConfigRechargeMapper">
  <select id="selectTConfigRechargeByFilter" resultType="com.ep.dao.model.generated.TConfigRecharge">
    SELECT * from t_config_recharge

    ORDER BY id
    <if test="filter.start != null and filter.size != null">
      limit #{filter.start}, #{filter.size}
    </if>
  </select>

  <select id="countTConfigRechargeByFilter" resultType="com.ep.dao.model.generated.TConfigRecharge">
    SELECT count(*) from t_config_recharge


  </select>
</mapper>